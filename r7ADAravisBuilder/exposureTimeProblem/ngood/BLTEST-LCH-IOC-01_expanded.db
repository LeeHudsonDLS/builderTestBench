# Updated rate calculation to use a smoothing factor w/ guard against negative values
record(calc, "BL99I-LCH-OAV-01:STAT:ArrayRate_RBV")
{
    field(INPA, "BL99I-LCH-OAV-01:STAT:ArrayRate_RBV.LB NPP NMS")   # Previous counter value
    field(INPB, "BL99I-LCH-OAV-01:STAT:ArrayCounter_RBV NPP NMS")   # Current counter value
    field(INPC, "1.0")                                # Delta time in seconds
    field(INPD, "BL99I-LCH-OAV-01:STAT:ArrayRate_RBV.VAL NPP NMS")  # Previous rate
    field(INPE, "0.0")                 # Smoothing factor
    field(CALC, "(D*E)/C+MAX(0,B-A)*(1-E)/C")
    field(PREC, "2" )
    field(EGU,  "Hz" )
    field(SCAN, "1 second")
}

