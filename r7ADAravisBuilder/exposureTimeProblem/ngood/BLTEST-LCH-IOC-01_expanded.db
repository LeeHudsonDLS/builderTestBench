record(sseq, "BL99I-LCH-OAV-01:PROC:ReadBackgroundTIFFSeq")
{
    # Make a backup copy of the NDArrayPort field
    field(DOL1, "BL99I-LCH-OAV-01:PROC:NDArrayPort")
    field(LNK1, "BL99I-LCH-OAV-01:PROC:PortBackup PP MS")
    # Set the NDArrayPort field to the TIFF plugin
    field(DOL2, "BL99I-LCH-OAV-01:PROC:TIFF:PortName_RBV")
    field(LNK2, "BL99I-LCH-OAV-01:PROC:NDArrayPort PP MS")
    # Enable array callbacks in the TIFF plugin
    field(STR3, "Enable")
    field(LNK3, "BL99I-LCH-OAV-01:PROC:TIFF:ArrayCallbacks PP MS")
    # Read the TIFF file
    field(DLY4, "0.1")
    field(DO4, "1")
    field(LNK4, "BL99I-LCH-OAV-01:PROC:TIFF:ReadFile PP MS")
    # Save background TIFF
    field(DLY5, "0.5")
    field(DO5, "1")
    field(LNK5, "BL99I-LCH-OAV-01:PROC:SaveBackground PP MS")
    # Restore the NDArrayPort field
    field(DOL6, "BL99I-LCH-OAV-01:PROC:PortBackup")
    field(LNK6, "BL99I-LCH-OAV-01:PROC:NDArrayPort PP MS")
}

