# Set a HIGH warning level for the input queue fill level to 75%
# of the full size of the queue
record(calcout, "BL99I-LCH-OAV-01:PROC:QueueUseHIGH") {
   field(CALC, "A*0.75")
   field(INPA, "BL99I-LCH-OAV-01:PROC:QueueSize")
   field(OUT,  "BL99I-LCH-OAV-01:PROC:QueueUse.HIGH")
   field(FLNK, "BL99I-LCH-OAV-01:PROC:QueueUseHIHI")
}

# Set a HIHI level for the input queue fill level to indicate an error
# (MAJOR alarm)  when it fills up to the full size of the queue (frames start dropping)
record(calcout, "BL99I-LCH-OAV-01:PROC:QueueUseHIHI") {
   field(CALC, "A*1.0")
   field(INPA, "BL99I-LCH-OAV-01:PROC:QueueSize")
   field(OUT,  "BL99I-LCH-OAV-01:PROC:QueueUse.HIHI")
}

